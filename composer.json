{
  "name": "robertboes/laravel-lti",
  "description": "A Laravel implementation for the LTI Tool Provider Library",
  "keywords": [
    "lti", "imsglobal"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Robert Boes",
      "email": "robert@boes.io",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.0",
    "illuminate/support": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*",
    "Illuminate/Database": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*",
    "imsglobal/lti": "3.0.2"
  },
  "autoload": {
    "psr-4": {
      "RobertBoes\\LaravelLti\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "RobertBoes\\LaravelLti\\ServiceProvider"
      ],
      "aliases": {
        "LTI": "RobertBoes\\LaravelLti\\Facade"
      }
    }
  }
}
